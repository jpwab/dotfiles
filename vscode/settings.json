{
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.roundedSelection": false,
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "visible",
    "editor.colorDecorators": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line-thin",
    "editor.selectionHighlight": false,
    "editor.showFoldingControls": "always",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.renderControlCharacters": true,
    "editor.lineNumbers": "on",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.fontSize": 16,
    "editor.fontFamily": "'IBM Plex Mono', monospace",
    "editor.fontLigatures": true,
    "breadcrumbs.enabled": false,
    "breadcrumbs.icons": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.statusBar.visible": true,
    "workbench.tips.enabled": false,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.colorTheme": "Solarized Light (no bold)",
    "workbench.editor.showIcons": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.iconTheme": null,
    "workbench.tree.indent": 14,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.colorCustomizations": {
        //"statusBar.background": "#F5F5F5",
        //"statusBar.foreground": "#575757",
        //"statusBar.noFolderBackground": "#212121",
        //"statusBar.debuggingBackground": "#263238",
        //"sideBar.background": "#F5F5F5",
        //"sideBar.foreground": "#575757",
        //"activityBar.background": "#F5F5F5",
        //"activityBar.foreground": "#575757",
        //        "editor.lineHighlightBa#ckground": "#e4e4e4"
    },
    "window.zoomLevel": 0,
    "window.openFilesInNewWindow": "off",
    "window.openFoldersInNewWindow": "off",
    "window.restoreFullscreen": false,
    "window.restoreWindows": "folders",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "files.associations": {
        "*.html": "html"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "[html]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "markdown-preview-enhanced.breakOnSingleNewLine": false,
    "terminal.integrated.fontFamily": "'IBM Plex Mono', monospace",
    "terminal.integrated.fontWeight": "300",
    "terminal.integrated.fontSize": 16,
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "goOutliner.extendExplorerTab": true,
    "goOutliner.enableDebugChannel": false,
    "go.autocompleteUnimportedPackages": true,
    "debug.console.fontSize": 16,
    "files.hotExit": "onExit",
    "bracket-pair-colorizer-2.colors": [
        "#FAC29A",
        "#B977DB",
        "#25B2BC"
    ],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
}