{
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "hidden",
    "editor.renderControlCharacters": true,
    "editor.lineNumbers": "on",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "goOutliner.extendExplorerTab": true,
    "goOutliner.enableDebugChannel": false,
    "terminal.integrated.fontSize": 16,
    "window.zoomLevel": 0,
    "git.autofetch": true,
    "files.associations": {
        "*.html": "html"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "[html]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "markdown-preview-enhanced.breakOnSingleNewLine": false,
    "workbench.editor.showIcons": false,
    "go.autocompleteUnimportedPackages": true,
    "workbench.statusBar.visible": true,
    "workbench.tips.enabled": false,
    "breadcrumbs.icons": false,
    "workbench.editor.closeOnFileDelete": true,
    // "workbench.colorCustomizations": {
    //     "tab.activeBackground": "#00000000", // transparent
    //     "titleBar.inactiveBackground": "#00000000",
    //     "editor.background": "#13141d",
    //     "editorGutter.background": "#13141d"
    // },
    "editor.fontSize": 16,
    "editor.fontFamily": "Fira Code",
    "editor.fontWeight": "400",
    "editor.fontLigatures": true,
    "debug.console.fontSize": 16,
    "git.enableSmartCommit": true,
    "window.restoreFullscreen": false,
    "window.restoreWindows": "folders",
    "files.hotExit": "onExit",
    "workbench.startupEditor": "newUntitledFile",
    "city-lights-icons-vsc.hidesExplorerArrows": false,
    "workbench.editor.revealIfOpen": true,
    "window.openFilesInNewWindow": "on",
    "window.openFoldersInNewWindow": "on",
    "workbench.iconTheme": "helium-icon-theme",
    "bracket-pair-colorizer-2.colors": [
        "#FAC29A",
        "#B977DB",
        "#25B2BC"
    ],
    "vscode-neovim.highlightGroups.unknownHighlight": {},
    "vscode-neovim.neovimPath": "/usr/local/bin/nvim",
    "workbench.colorTheme": "GitHub Light",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    },
    "material-icon-theme.showWelcomeMessage": false
}